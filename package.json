		"clean": "gulp clean",
		"cleanall": "gulp cleanall",
		"dev": "run-p watch gulpstart",
		"format": "gulp format",
		"gulp": "gulp build",
		"gulpstart": "run-s gulp start",
		"init": "npm run migrate",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --fix 'src/**'",
		"migrate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
		"migrate:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
		"migrate:gen": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
		"migrateandstart": "npm run migrate && npm run start",
		"ormconfig": "node ./built/ormconfig.js",
		"start": "node ./index.js",
		"start-product": "cross-env NODE_ENV=production node ./index.js",
		"test": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_COMPILER_OPTIONS=\"{\\\"target\\\":\\\"es2017\\\",\\\"module\\\":\\\"commonjs\\\",\\\"typeRoots\\\":[\\\"node_modules/@types\\\",\\\"src/@types\\\"]}\" mocha",
